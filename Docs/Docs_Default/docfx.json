{
  "build": {
    "content":
      [
        {
          "files": ["**/**.md", "**/**.yml", "**/**.json"],
          "exclude": ["**/obj/**"]
        }
      ],
    "resource": [
        {
          "files": ["**/images/**"],
          "exclude": ["**/obj/**"]
        }
    ],
    "globalMetadata": {
        "layout": "Conceptual",
        "ROBOTS": "INDEX, FOLLOW",
        "breadcrumb_path": "~/toc.json"
    },
    "externalReference": [
    ],
    "template": "op.html",
    "dest": "Docs_Default"
  }
}